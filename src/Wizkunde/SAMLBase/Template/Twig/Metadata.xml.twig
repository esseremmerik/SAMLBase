<md:EntityDescriptor
        xmlns:md="urn:oasis:names:tc:SAML:2.0:metadata"
        xmlns:saml="urn:oasis:names:tc:SAML:2.0:assertion"
        xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
        entityID="{{ EntityID }}">
    <!-- insert ds:Signature element -->
    <md:SPSSODescriptor
            protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
        <md:KeyDescriptor use="signing">
            <ds:KeyInfo>
                <ds:KeyName>SP SSO Key</ds:KeyName>
            </ds:KeyInfo>
        </md:KeyDescriptor>
        <md:ArtifactResolutionService isDefault="true" index="0"
                                      Binding="urn:oasis:names:tc:SAML:2.0:bindings:SOAP"
                                      Location="{{ postURL }}"/>
        <md:NameIDFormat>
            urn:oasis:names:tc:SAML:2.0:nameid-format:persistent
        </md:NameIDFormat>
        <md:AssertionConsumerService isDefault="true" index="0"
                                     Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
                                     Location="{{ postURL }}"/>
        <md:AssertionConsumerService index="1"
                                     Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Artifact"
                                     Location="{{ postURL }}"/>
        <md:AttributeConsumingService isDefault="true" index="1">
            <md:ServiceName xml:lang="en">
                {{ OrganizationName }}
            </md:ServiceName>
            <md:RequestedAttribute
                    NameFormat="urn:oasis:names:tc:SAML:2.0:attrname-format:uri"
                    Name="urn:oid:1.3.6.1.4.1.5923.1.1.1.1"
                    FriendlyName="eduPersonAffiliation">
            </md:RequestedAttribute>
        </md:AttributeConsumingService>
    </md:SPSSODescriptor>
    <md:Organization>
        <md:OrganizationName xml:lang="en">
            {{ OrganizationName }}
        </md:OrganizationName>
        <md:OrganizationDisplayName xml:lang="en">
            {{ OrganizationDisplayName }}
        </md:OrganizationDisplayName>
        <md:OrganizationURL xml:lang="en">
            {{ OrganizationURL }}
        </md:OrganizationURL>
    </md:Organization>
    <md:ContactPerson contactType="technical">
        <md:SurName>{{ ContactPersonSurName }}</md:SurName>
        <md:EmailAddress>{{ ContactPersonEmailAddress }}</md:EmailAddress>
    </md:ContactPerson>
</md:EntityDescriptor>